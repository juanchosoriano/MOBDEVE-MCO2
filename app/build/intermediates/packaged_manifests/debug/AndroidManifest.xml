<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mobdeve.s11.soriano.juancho.mco2memobile"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="23"
        android:targetSdkVersion="30" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:extractNativeLibs="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.Memobile" >
        <activity
            android:name="com.mobdeve.s11.soriano.juancho.mco2memobile.MainActivity"
            android:exported="true"
            android:label=""
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name="com.mobdeve.s11.soriano.juancho.mco2memobile.RegisterActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name="com.mobdeve.s11.soriano.juancho.mco2memobile.LoginActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" >

            <!-- App is set to always launch on login screen for the demo. -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mobdeve.s11.soriano.juancho.mco2memobile.AddNoteActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name="com.mobdeve.s11.soriano.juancho.mco2memobile.AddItemActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name="com.mobdeve.s11.soriano.juancho.mco2memobile.AddEventActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name="com.mobdeve.s11.soriano.juancho.mco2memobile.AddFriendsActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name="com.mobdeve.s11.soriano.juancho.mco2memobile.SettingsActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name="com.mobdeve.s11.soriano.juancho.mco2memobile.ViewFriendsActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>